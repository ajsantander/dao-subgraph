{
  "name": "@aragon/dao-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/aragon-network",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "create:rpc": "graph create dao-subgraph --node http://127.0.0.1:8020",
    "build": "graph build",
    "deploy:rpc": "graph deploy dao-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy": "graph deploy --debug dao-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "watch-local": "graph deploy dao-subgraph --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0"
  }
}
